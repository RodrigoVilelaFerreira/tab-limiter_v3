{
  "name": "Limitador de Tabs",
  "description": "Limita o número de tabs do google chrome em 5",
  "version": "0.3.0",
  "author": "Matthias Vogt, edição de Rodrio Ferreira",
  "homepage_url": "https://github.com/matthias-vogt/tab-limiter",
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": [
    "storage",
    "notifications"
  ],
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "default_title": "Limitador de Tabs",
    "default_popup": "options.html"
  },
  "content_security_policy": {}
}