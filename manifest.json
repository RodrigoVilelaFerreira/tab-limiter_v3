{
  "name": "Tab Limiter",
  "description": "Limit the number of open tabs \u00e2\u20ac\u201c in total and per window",
  "version": "0.3.0",
  "author": "Matthias Vogt",
  "homepage_url": "https://github.com/matthias-vogt/tab-limiter",
  "icons": {
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "service_worker.js"
  },
  "permissions": [
    "storage"
  ],
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "default_popup": "options.html",
    "default_title": "Tab Limiter"
  },
  "content_security_policy": {}
}